  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  },
  "typings": "index.d.ts",
  "name": "@terra-dev/readonly-wallet",
  "version": "2.5.3",
  "dependencies": {
    "@terra-dev/wallet-types": "^2.5.3"
  },
  "main": "./_commonjs/index.js",
  "module": "./index.js",
  "peerDependencies": {
    "@terra-money/terra.js": "^2.0.0"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./_commonjs/index.js",
      "node": "./_commonjs/index.js",
      "default": "./index.js"
    },
    "./public/*": "./public/*",
    "./connect": {
      "import": "./connect.js",
      "require": "./_commonjs/connect.js",
      "node": "./_commonjs/connect.js",
      "default": "./connect.js"
    },
    "./connect.js": {
      "import": "./connect.js",
      "require": "./_commonjs/connect.js",
      "node": "./_commonjs/connect.js",
      "default": "./connect.js"
    },
    "./README.md": "./README.md",
    "./storage": {
      "import": "./storage.js",
      "require": "./_commonjs/storage.js",
      "node": "./_commonjs/storage.js",
      "default": "./storage.js"
    },
    "./storage.js": {
      "import": "./storage.js",
      "require": "./_commonjs/storage.js",
      "node": "./_commonjs/storage.js",
      "default": "./storage.js"
    },
    "./types": {
      "import": "./types.js",
      "require": "./_commonjs/types.js",
      "node": "./_commonjs/types.js",
      "default": "./types.js"
    },
    "./types.js": {
      "import": "./types.js",
      "require": "./_commonjs/types.js",
      "node": "./_commonjs/types.js",
      "default": "./types.js"
    }
  }
}
